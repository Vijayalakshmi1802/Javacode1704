package abstractExample;

//AbstractionÂ is a process of hiding the implementation details and showing only functionality to the user.

abstract class Bank 
{    
abstract int getRateOfInterest();    
}    
class SBI extends Bank
{    
int getRateOfInterest()
{
	return 7;
}    
}    
class HSBC extends Bank
{    
int getRateOfInterest(){return 8;}    
}    
    
class TestBank{    
public static void main(String args[]){    
Bank b;  
b=new SBI();  
System.out.println("SBI Rate of Interest is: "+b.getRateOfInterest()+" %");    
b=new HSBC();  
System.out.println("HSBC Rate of Interest is: "+b.getRateOfInterest()+" %");    
}}    
